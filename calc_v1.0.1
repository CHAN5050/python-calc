#!python

#get numbers...
num1 = float(input("First number: "));
op = input("Operator: ");
num2 = float(input("Second number: "));

#calc
if op == "/":
  print(num1 / num2);
elif op == "*":
  print(num1 * num2);
elif op == "-":
  print(num1 - num2);
elif op == "+":
  print(num1 + num2);
else:
  print("Unknown Operator");

#end
print("");
input("Enter to end");
exit();
